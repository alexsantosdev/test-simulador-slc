                    <xs:documentation>
                        <cat:InfArquivo>
                            <cat:Descricao>
                              	Destinado ao credenciador informar ao SLC as liquidações das transações de cartões de crédito.
                            </cat:Descricao>
                            <cat:Emissor>Credenciador</cat:Emissor>
                            <cat:Destinatario>SLC</cat:Destinatario>
                        </cat:InfArquivo>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ASLC027RET" type="ASLC027RETComplexType">
                <xs:annotation>
                    <xs:documentation>
                        <cat:InfArquivo>
                            <cat:Descricao>
                              	Destinado ao SLC retornar o processamento das liquidações das transações de cartões de crédito
                            </cat:Descricao>
                            <cat:Emissor>SLC</cat:Emissor>
                            <cat:Destinatario>Credenciador</cat:Destinatario>
                        </cat:InfArquivo>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>

