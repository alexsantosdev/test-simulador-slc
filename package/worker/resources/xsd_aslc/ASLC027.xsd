    <!-- Definição do ASLCDOC -->
    <xs:element name="ASLCDOC" type="ASLCDOCComplexType">
        <xs:annotation>
            <xs:appinfo>
                <cat:InfEvento>
                    <cat:Evento>
                      	ASLC027 – Credenciador informa as liquidações das transações de cartões de crédito
                    </cat:Evento>
                    <cat:Descricao>
                      	Destinado ao credenciador informar ao SLC as liquidações das transações de cartões de crédito.
                    </cat:Descricao>
                </cat:InfEvento>
            </xs:appinfo>
        </xs:annotation>
    </xs:element>

    <!-- Definição do ASLCDOC do arquivo -->
